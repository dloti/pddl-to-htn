(define (problem p)
(:domain blocks)
(:objects
	D - BLOCK
	B - BLOCK
	A - BLOCK
	C - BLOCK
	D - OBJECT
	B - OBJECT
	A - OBJECT
	C - OBJECT
)
(:htn
	:tasks (and
	(task0 (ACHIEVE-ON D C))
	(task1 (ACHIEVE-ON C B))
	(task2 (ACHIEVE-ON B A))
	)
:ordering (and
	(task2 < task1)
	(task1 < task0)
)
:constraints ( )
)
(:init
        (CLEAR C)
        (CLEAR A)
        (CLEAR B)
        (CLEAR D)
        (ONTABLE C)
        (ONTABLE A)
        (ONTABLE B)
        (ONTABLE D)
        (HANDEMPTY)
)
)

