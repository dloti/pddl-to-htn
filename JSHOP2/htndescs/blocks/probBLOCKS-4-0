
(DEFPROBLEM PROBLEM blocks
    (
        (BLOCK D)
        (BLOCK B)
        (BLOCK A)
        (BLOCK C)
        (OBJECT D)
        (OBJECT B)
        (OBJECT A)
        (OBJECT C)
        (CLEAR C)
        (CLEAR A)
        (CLEAR B)
        (CLEAR D)
        (ONTABLE C)
        (ONTABLE A)
        (ONTABLE B)
        (ONTABLE D)
        (HANDEMPTY)
    )
    (
        (ACHIEVE-ON D C)
        (ACHIEVE-ON C B)
        (ACHIEVE-ON B A)
	)
)

