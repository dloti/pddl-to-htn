(define (problem p)
(:domain blocks)
(:objects
	D - BLOCK
	B - BLOCK
	A - BLOCK
	C - BLOCK
	D - OBJECT
	B - OBJECT
	A - OBJECT
	C - OBJECT
)
(:htn
	:tasks (and
	(ACHIEVE-ON D C)
	(ACHIEVE-ON C B)
	(ACHIEVE-ON B A)
	)
:ordering ( )
:constraints ( )
)
(:init
        (CLEAR C)
        (CLEAR A)
        (CLEAR B)
        (CLEAR D)
        (ONTABLE C)
        (ONTABLE A)
        (ONTABLE B)
        (ONTABLE D)
        (HANDEMPTY)
)
)

