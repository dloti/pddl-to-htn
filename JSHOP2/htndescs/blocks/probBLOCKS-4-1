
(DEFPROBLEM PROBLEM blocks
    (
        (BLOCK A)
        (BLOCK C)
        (BLOCK D)
        (BLOCK B)
        (CLEAR B)
        (ONTABLE D)
        (ON B C)
        (ON C A)
        (ON A D)
        (HANDEMPTY)
    )
    (
        (ACHIEVE-ON D C)
        (ACHIEVE-ON C A)
        (ACHIEVE-ON A B)
	)
)

