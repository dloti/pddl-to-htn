(define (problem p)
(:domain blocks)
(:objects
	A - BLOCK
	C - BLOCK
	D - BLOCK
	B - BLOCK
	A - OBJECT
	C - OBJECT
	D - OBJECT
	B - OBJECT
)
(:htn
	:tasks (and
	(task0 (ACHIEVE-ON D C))
	(task1 (ACHIEVE-ON C A))
	(task2 (ACHIEVE-ON A B))
	)
:ordering (and
	(task2 < task1)
	(task1 < task0)
)
:constraints ( )
)
(:init
        (CLEAR B)
        (ONTABLE D)
        (ON B C)
        (ON C A)
        (ON A D)
        (HANDEMPTY)
)
)

