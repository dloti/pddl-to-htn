
(DEFPROBLEM PROBLEM blocks
    (
        (BLOCK B)
        (BLOCK D)
        (BLOCK C)
        (BLOCK A)
        (CLEAR A)
        (CLEAR C)
        (CLEAR D)
        (ONTABLE A)
        (ONTABLE B)
        (ONTABLE D)
        (ON C B)
        (HANDEMPTY)
    )
    (
        (ACHIEVE-ON A B)
        (ACHIEVE-ON B C)
        (ACHIEVE-ON C D)
	)
)

