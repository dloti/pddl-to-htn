(define (problem p)
(:domain blocks)
(:objects
	B - BLOCK
	D - BLOCK
	C - BLOCK
	A - BLOCK
	B - OBJECT
	D - OBJECT
	C - OBJECT
	A - OBJECT
)
(:htn
	:tasks (and
	(ACHIEVE-ON A B)
	(ACHIEVE-ON B C)
	(ACHIEVE-ON C D)
	)
:ordering ( )
:constraints ( )
)
(:init
        (CLEAR A)
        (CLEAR C)
        (CLEAR D)
        (ONTABLE A)
        (ONTABLE B)
        (ONTABLE D)
        (ON C B)
        (HANDEMPTY)
)
)

