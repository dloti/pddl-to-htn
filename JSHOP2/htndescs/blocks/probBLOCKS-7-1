(define (problem p)
(:domain blocks)
(:objects
	E - BLOCK
	B - BLOCK
	D - BLOCK
	F - BLOCK
	G - BLOCK
	C - BLOCK
	A - BLOCK
	E - OBJECT
	B - OBJECT
	D - OBJECT
	F - OBJECT
	G - OBJECT
	C - OBJECT
	A - OBJECT
)
(:htn
	:tasks (and
	(ACHIEVE-ON A E)
	(ACHIEVE-ON E B)
	(ACHIEVE-ON B F)
	(ACHIEVE-ON F G)
	(ACHIEVE-ON G C)
	(ACHIEVE-ON C D)
	)
:ordering ( )
:constraints ( )
)
(:init
        (CLEAR A)
        (CLEAR C)
        (ONTABLE G)
        (ONTABLE F)
        (ON A G)
        (ON C D)
        (ON D B)
        (ON B E)
        (ON E F)
        (HANDEMPTY)
)
)

