(define (problem p)
(:domain satellite)
(:objects
	SATELLITE0 - SATELLITE
	SATELLITE1 - SATELLITE
	STAR1 - DIRECTION
	STAR2 - DIRECTION
	STAR0 - DIRECTION
	STAR3 - DIRECTION
	STAR4 - DIRECTION
	PHENOMENON5 - DIRECTION
	PHENOMENON6 - DIRECTION
	PHENOMENON7 - DIRECTION
	INSTRUMENT0 - INSTRUMENT
	INSTRUMENT1 - INSTRUMENT
	INSTRUMENT2 - INSTRUMENT
	INSTRUMENT3 - INSTRUMENT
	IMAGE1 - MODE
	INFRARED0 - MODE
	SPECTROGRAPH2 - MODE
	SATELLITE0 - OBJECT
	INSTRUMENT0 - OBJECT
	INSTRUMENT1 - OBJECT
	INSTRUMENT2 - OBJECT
	SATELLITE1 - OBJECT
	INSTRUMENT3 - OBJECT
	IMAGE1 - OBJECT
	INFRARED0 - OBJECT
	SPECTROGRAPH2 - OBJECT
	STAR1 - OBJECT
	STAR2 - OBJECT
	STAR0 - OBJECT
	STAR3 - OBJECT
	STAR4 - OBJECT
	PHENOMENON5 - OBJECT
	PHENOMENON6 - OBJECT
	PHENOMENON7 - OBJECT
)
(:htn
	:tasks (and
	(task0 (ACHIEVE-POINTING SATELLITE0 PHENOMENON5))
	(task1 (ACHIEVE-HAVE_IMAGE STAR3 INFRARED0))
	(task2 (ACHIEVE-HAVE_IMAGE STAR4 SPECTROGRAPH2))
	(task3 (ACHIEVE-HAVE_IMAGE PHENOMENON5 SPECTROGRAPH2))
	(task4 (ACHIEVE-HAVE_IMAGE PHENOMENON7 SPECTROGRAPH2))
	)
:ordering (and
	(task4 < task3)
	(task3 < task2)
	(task2 < task1)
	(task1 < task0)
)
:constraints ( )
)
(:init
        (SUPPORTS INSTRUMENT0 SPECTROGRAPH2)
        (SUPPORTS INSTRUMENT0 INFRARED0)
        (CALIBRATION_TARGET INSTRUMENT0 STAR1)
        (SUPPORTS INSTRUMENT1 IMAGE1)
        (CALIBRATION_TARGET INSTRUMENT1 STAR2)
        (SUPPORTS INSTRUMENT2 INFRARED0)
        (SUPPORTS INSTRUMENT2 IMAGE1)
        (CALIBRATION_TARGET INSTRUMENT2 STAR0)
        (ON_BOARD INSTRUMENT0 SATELLITE0)
        (ON_BOARD INSTRUMENT1 SATELLITE0)
        (ON_BOARD INSTRUMENT2 SATELLITE0)
        (POWER_AVAIL SATELLITE0)
        (POINTING SATELLITE0 STAR4)
        (SUPPORTS INSTRUMENT3 SPECTROGRAPH2)
        (SUPPORTS INSTRUMENT3 INFRARED0)
        (SUPPORTS INSTRUMENT3 IMAGE1)
        (CALIBRATION_TARGET INSTRUMENT3 STAR0)
        (ON_BOARD INSTRUMENT3 SATELLITE1)
        (POWER_AVAIL SATELLITE1)
        (POINTING SATELLITE1 STAR0)
)
)

