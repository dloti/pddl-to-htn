(define (problem p)
(:domain satellite)
(:objects
	SATELLITE0 - SATELLITE
	STAR0 - DIRECTION
	GROUNDSTATION1 - DIRECTION
	GROUNDSTATION2 - DIRECTION
	PHENOMENON3 - DIRECTION
	PHENOMENON4 - DIRECTION
	STAR5 - DIRECTION
	PHENOMENON6 - DIRECTION
	INSTRUMENT0 - INSTRUMENT
	IMAGE1 - MODE
	SPECTROGRAPH2 - MODE
	THERMOGRAPH0 - MODE
	SATELLITE0 - OBJECT
	INSTRUMENT0 - OBJECT
	IMAGE1 - OBJECT
	SPECTROGRAPH2 - OBJECT
	THERMOGRAPH0 - OBJECT
	STAR0 - OBJECT
	GROUNDSTATION1 - OBJECT
	GROUNDSTATION2 - OBJECT
	PHENOMENON3 - OBJECT
	PHENOMENON4 - OBJECT
	STAR5 - OBJECT
	PHENOMENON6 - OBJECT
)
(:htn
	:tasks (and
	(task0 (ACHIEVE-HAVE_IMAGE PHENOMENON4 THERMOGRAPH0))
	(task1 (ACHIEVE-HAVE_IMAGE STAR5 THERMOGRAPH0))
	(task2 (ACHIEVE-HAVE_IMAGE PHENOMENON6 THERMOGRAPH0))
	)
:ordering (and
	(task2 < task1)
	(task1 < task0)
)
:constraints ( )
)
(:init
        (SUPPORTS INSTRUMENT0 THERMOGRAPH0)
        (CALIBRATION_TARGET INSTRUMENT0 GROUNDSTATION2)
        (ON_BOARD INSTRUMENT0 SATELLITE0)
        (POWER_AVAIL SATELLITE0)
        (POINTING SATELLITE0 PHENOMENON6)
)
)

