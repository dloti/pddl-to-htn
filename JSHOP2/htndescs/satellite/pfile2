(define (problem p)
(:domain satellite)
(:objects
	SATELLITE0 - SATELLITE
	GROUNDSTATION1 - DIRECTION
	STAR0 - DIRECTION
	GROUNDSTATION2 - DIRECTION
	PLANET3 - DIRECTION
	PLANET4 - DIRECTION
	PHENOMENON5 - DIRECTION
	PHENOMENON6 - DIRECTION
	STAR7 - DIRECTION
	INSTRUMENT0 - INSTRUMENT
	INSTRUMENT1 - INSTRUMENT
	INFRARED0 - MODE
	INFRARED1 - MODE
	IMAGE2 - MODE
	SATELLITE0 - OBJECT
	INSTRUMENT0 - OBJECT
	INSTRUMENT1 - OBJECT
	INFRARED0 - OBJECT
	INFRARED1 - OBJECT
	IMAGE2 - OBJECT
	GROUNDSTATION1 - OBJECT
	STAR0 - OBJECT
	GROUNDSTATION2 - OBJECT
	PLANET3 - OBJECT
	PLANET4 - OBJECT
	PHENOMENON5 - OBJECT
	PHENOMENON6 - OBJECT
	STAR7 - OBJECT
)
(:htn
	:tasks (and
	(task0 (ACHIEVE-HAVE_IMAGE PLANET3 INFRARED0))
	(task1 (ACHIEVE-HAVE_IMAGE PLANET4 INFRARED0))
	(task2 (ACHIEVE-HAVE_IMAGE PHENOMENON5 IMAGE2))
	(task3 (ACHIEVE-HAVE_IMAGE PHENOMENON6 INFRARED0))
	(task4 (ACHIEVE-HAVE_IMAGE STAR7 INFRARED0))
	)
:ordering (and
	(task4 < task3)
	(task3 < task2)
	(task2 < task1)
	(task1 < task0)
)
:constraints ( )
)
(:init
        (SUPPORTS INSTRUMENT0 INFRARED1)
        (SUPPORTS INSTRUMENT0 INFRARED0)
        (CALIBRATION_TARGET INSTRUMENT0 STAR0)
        (SUPPORTS INSTRUMENT1 IMAGE2)
        (SUPPORTS INSTRUMENT1 INFRARED1)
        (SUPPORTS INSTRUMENT1 INFRARED0)
        (CALIBRATION_TARGET INSTRUMENT1 GROUNDSTATION2)
        (ON_BOARD INSTRUMENT0 SATELLITE0)
        (ON_BOARD INSTRUMENT1 SATELLITE0)
        (POWER_AVAIL SATELLITE0)
        (POINTING SATELLITE0 PLANET4)
)
)

