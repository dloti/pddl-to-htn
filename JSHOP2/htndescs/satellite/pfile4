(define (problem p)
(:domain satellite)
(:objects
	SATELLITE0 - SATELLITE
	SATELLITE1 - SATELLITE
	GROUNDSTATION1 - DIRECTION
	STAR0 - DIRECTION
	STAR2 - DIRECTION
	PLANET3 - DIRECTION
	STAR4 - DIRECTION
	PLANET5 - DIRECTION
	STAR6 - DIRECTION
	STAR7 - DIRECTION
	PHENOMENON8 - DIRECTION
	PHENOMENON9 - DIRECTION
	INSTRUMENT0 - INSTRUMENT
	INSTRUMENT1 - INSTRUMENT
	INSTRUMENT2 - INSTRUMENT
	INFRARED0 - MODE
	INFRARED1 - MODE
	THERMOGRAPH2 - MODE
	SATELLITE0 - OBJECT
	INSTRUMENT0 - OBJECT
	SATELLITE1 - OBJECT
	INSTRUMENT1 - OBJECT
	INSTRUMENT2 - OBJECT
	INFRARED0 - OBJECT
	INFRARED1 - OBJECT
	THERMOGRAPH2 - OBJECT
	GROUNDSTATION1 - OBJECT
	STAR0 - OBJECT
	STAR2 - OBJECT
	PLANET3 - OBJECT
	STAR4 - OBJECT
	PLANET5 - OBJECT
	STAR6 - OBJECT
	STAR7 - OBJECT
	PHENOMENON8 - OBJECT
	PHENOMENON9 - OBJECT
)
(:htn
	:tasks (and
	(task0 (ACHIEVE-POINTING SATELLITE1 PLANET5))
	(task1 (ACHIEVE-HAVE_IMAGE PLANET3 INFRARED1))
	(task2 (ACHIEVE-HAVE_IMAGE STAR4 INFRARED1))
	(task3 (ACHIEVE-HAVE_IMAGE PLANET5 THERMOGRAPH2))
	(task4 (ACHIEVE-HAVE_IMAGE STAR6 INFRARED1))
	(task5 (ACHIEVE-HAVE_IMAGE STAR7 INFRARED0))
	(task6 (ACHIEVE-HAVE_IMAGE PHENOMENON8 THERMOGRAPH2))
	(task7 (ACHIEVE-HAVE_IMAGE PHENOMENON9 INFRARED0))
	)
:ordering (and
	(task7 < task6)
	(task6 < task5)
	(task5 < task4)
	(task4 < task3)
	(task3 < task2)
	(task2 < task1)
	(task1 < task0)
)
:constraints ( )
)
(:init
        (SUPPORTS INSTRUMENT0 THERMOGRAPH2)
        (SUPPORTS INSTRUMENT0 INFRARED0)
        (CALIBRATION_TARGET INSTRUMENT0 STAR0)
        (ON_BOARD INSTRUMENT0 SATELLITE0)
        (POWER_AVAIL SATELLITE0)
        (POINTING SATELLITE0 STAR6)
        (SUPPORTS INSTRUMENT1 INFRARED0)
        (SUPPORTS INSTRUMENT1 THERMOGRAPH2)
        (SUPPORTS INSTRUMENT1 INFRARED1)
        (CALIBRATION_TARGET INSTRUMENT1 STAR2)
        (SUPPORTS INSTRUMENT2 THERMOGRAPH2)
        (SUPPORTS INSTRUMENT2 INFRARED1)
        (CALIBRATION_TARGET INSTRUMENT2 STAR2)
        (ON_BOARD INSTRUMENT1 SATELLITE1)
        (ON_BOARD INSTRUMENT2 SATELLITE1)
        (POWER_AVAIL SATELLITE1)
        (POINTING SATELLITE1 STAR0)
)
)

