(define (domain satellite
(:requirements :typing)
(:types
)
(:predicates
	( ON_BOARD ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE )
	( SUPPORTS ?INSTRUMENT0 - INSTRUMENT ?MODE1 - MODE )
	( POINTING ?SATELLITE0 - SATELLITE ?DIRECTION1 - DIRECTION )
	( POWER_AVAIL ?SATELLITE0 - SATELLITE )
	( POWER_ON ?INSTRUMENT0 - INSTRUMENT )
	( CALIBRATED ?INSTRUMENT0 - INSTRUMENT )
	( HAVE_IMAGE ?DIRECTION0 - DIRECTION ?MODE1 - MODE )
	( CALIBRATION_TARGET ?INSTRUMENT0 - INSTRUMENT ?DIRECTION1 - DIRECTION )
)
( :action !TURN_TO
	:parameters ( ?SATELLITE0 - SATELLITE ?DIRECTION1 - DIRECTION ?DIRECTION2 - DIRECTION)
	:precondition (and
		 ( POINTING ?SATELLITE0 ?DIRECTION2 ))
	:effect (and
		 ( NOT ( POINTING ?SATELLITE0 ?DIRECTION2 ) ) ( POINTING ?SATELLITE0 ?DIRECTION1 ))
)
( :action !SWITCH_ON
	:parameters ( ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE)
	:precondition (and
		 ( ON_BOARD ?INSTRUMENT0 ?SATELLITE1 ) ( POWER_AVAIL ?SATELLITE1 ))
	:effect (and
		 ( NOT ( CALIBRATED ?INSTRUMENT0 ) ) ( NOT ( POWER_AVAIL ?SATELLITE1 ) ) ( POWER_ON ?INSTRUMENT0 ))
)
( :action !SWITCH_OFF
	:parameters ( ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE)
	:precondition (and
		 ( ON_BOARD ?INSTRUMENT0 ?SATELLITE1 ) ( POWER_ON ?INSTRUMENT0 ))
	:effect (and
		 ( NOT ( POWER_ON ?INSTRUMENT0 ) ) ( POWER_AVAIL ?SATELLITE1 ))
)
( :action !CALIBRATE
	:parameters ( ?SATELLITE0 - SATELLITE ?INSTRUMENT1 - INSTRUMENT ?DIRECTION2 - DIRECTION)
	:precondition (and
		 ( ON_BOARD ?INSTRUMENT1 ?SATELLITE0 ) ( CALIBRATION_TARGET ?INSTRUMENT1 ?DIRECTION2 ) ( POINTING ?SATELLITE0 ?DIRECTION2 ) ( POWER_ON ?INSTRUMENT1 ))
	:effect (and
		 ( CALIBRATED ?INSTRUMENT1 ))
)
( :action !TAKE_IMAGE
	:parameters ( ?SATELLITE0 - SATELLITE ?DIRECTION1 - DIRECTION ?INSTRUMENT2 - INSTRUMENT ?MODE3 - MODE)
	:precondition (and
		 ( CALIBRATED ?INSTRUMENT2 ) ( ON_BOARD ?INSTRUMENT2 ?SATELLITE0 ) ( SUPPORTS ?INSTRUMENT2 ?MODE3 ) ( POWER_ON ?INSTRUMENT2 ) ( POINTING ?SATELLITE0 ?DIRECTION1 ))
	:effect (and
		 ( HAVE_IMAGE ?DIRECTION1 ?MODE3 ))
)
(:task DO-POWER_AVAIL-SWITCH_OFF1
	:parameters ( ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE )
)
(:method M-DO-POWER_AVAIL-SWITCH_OFF1
	:parameters ( ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE )
	:task ( DO-POWER_AVAIL-SWITCH_OFF1 ?INSTRUMENT0 ?SATELLITE1 )
	:precondition (and  ( NOT ( POWER_AVAIL ?SATELLITE1 ) ))
	:ordered-subtasks (and  ( ACHIEVE-POWER_ON ?INSTRUMENT0 ) ( !SWITCH_OFF ?INSTRUMENT0 ?SATELLITE1 ) )
)
(:task DO-POWER_ON-SWITCH_ON2
	:parameters ( ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE )
)
(:method M-DO-POWER_ON-SWITCH_ON2
	:parameters ( ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE )
	:task ( DO-POWER_ON-SWITCH_ON2 ?INSTRUMENT0 ?SATELLITE1 )
	:precondition (and  ( NOT ( POWER_ON ?INSTRUMENT0 ) ))
	:ordered-subtasks (and  ( ACHIEVE-POWER_AVAIL ?SATELLITE1 ) ( !SWITCH_ON ?INSTRUMENT0 ?SATELLITE1 ) )
)
(:task DO-CALIBRATED-SWITCH_ON3
	:parameters ( ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE )
)
(:method M-DO-CALIBRATED-SWITCH_ON3
	:parameters ( ?INSTRUMENT0 - INSTRUMENT ?SATELLITE1 - SATELLITE )
	:task ( DO-CALIBRATED-SWITCH_ON3 ?INSTRUMENT0 ?SATELLITE1 )
	:precondition (and  ( NOT ( CALIBRATED ?INSTRUMENT0 ) ))
	:ordered-subtasks (and  ( ACHIEVE-POWER_AVAIL ?SATELLITE1 ) ( !SWITCH_ON ?INSTRUMENT0 ?SATELLITE1 ) )
)
(:task DO-CALIBRATED-CALIBRATE3
	:parameters ( ?SATELLITE0 - SATELLITE ?INSTRUMENT1 - INSTRUMENT ?DIRECTION2 - DIRECTION )
)
(:method M-DO-CALIBRATED-CALIBRATE3
	:parameters ( ?SATELLITE0 - SATELLITE ?INSTRUMENT1 - INSTRUMENT ?DIRECTION2 - DIRECTION )
	:task ( DO-CALIBRATED-CALIBRATE3 ?SATELLITE0 ?INSTRUMENT1 ?DIRECTION2 )
	:precondition (and  ( NOT ( CALIBRATED ?INSTRUMENT1 ) ))
	:ordered-subtasks (and  ( ACHIEVE-POWER_ON ?INSTRUMENT1 ) ( ACHIEVE-POINTING ?SATELLITE0 ?DIRECTION2 ) ( !CALIBRATE ?SATELLITE0 ?INSTRUMENT1 ?DIRECTION2 ) )
)
(:task DO-HAVE_IMAGE-TAKE_IMAGE4
	:parameters ( ?SATELLITE0 - SATELLITE ?DIRECTION1 - DIRECTION ?INSTRUMENT2 - INSTRUMENT ?MODE3 - MODE )
)
(:method M-DO-HAVE_IMAGE-TAKE_IMAGE4
	:parameters ( ?SATELLITE0 - SATELLITE ?DIRECTION1 - DIRECTION ?INSTRUMENT2 - INSTRUMENT ?MODE3 - MODE )
	:task ( DO-HAVE_IMAGE-TAKE_IMAGE4 ?SATELLITE0 ?DIRECTION1 ?INSTRUMENT2 ?MODE3 )
	:precondition (and  ( NOT ( HAVE_IMAGE ?DIRECTION1 ?MODE3 ) ))
	:ordered-subtasks (and  ( ACHIEVE-POWER_ON ?INSTRUMENT2 ) ( ACHIEVE-CALIBRATED ?INSTRUMENT2 ) ( ACHIEVE-POINTING ?SATELLITE0 ?DIRECTION1 ) ( !TAKE_IMAGE ?SATELLITE0 ?DIRECTION1 ?INSTRUMENT2 ?MODE3 ) )
)
    ( :METHOD ( ACHIEVE-POINTING ?SATELLITE0 ?DIRECTION1 )
              ( ( LOCKED-POINTING ?SATELLITE0 ?DIRECTION1 ) ( SATELLITE ?SATELLITE0 )  ( DIRECTION ?DIRECTION1 )  )
              ( ( !!FLAG-POINTING ?SATELLITE0 ?DIRECTION1 ) )
   )
    ( :METHOD ( ACHIEVE-POINTING ?SATELLITE0 ?DIRECTION1 )
              ( ( POINTING ?SATELLITE0 ?DIRECTION1 ) ( SATELLITE ?SATELLITE0 )  ( DIRECTION ?DIRECTION1 )  ( NOT ( LOCKED-POINTING ?SATELLITE0 ?DIRECTION1 ) ) )
              ( ( !!LOCK-POINTING ?SATELLITE0 ?DIRECTION1 ) )
    )
    ( :METHOD ( ACHIEVE-POINTING ?SATELLITE0 ?DIRECTION1 )
              ( ( NOT ( ACHIEVING-POINTING ?SATELLITE0 ?DIRECTION1 ) ) ( NOT ( POINTING ?SATELLITE0 ?DIRECTION1 ) ) ( SATELLITE ?SATELLITE0 )  ( DIRECTION ?DIRECTION1 )  ( NOT ( LOCKED-POINTING ?SATELLITE0 ?DIRECTION1 ) ) )
              ( ( !!ACHIEVE0 ?SATELLITE0 ) ( ACHIEVE-POINTING0 ?SATELLITE0 ?DIRECTION1 ) ( !!LOCK-POINTING ?SATELLITE0 ?DIRECTION1 ) ( !!STOPALL0 ?SATELLITE0 ) )
    )
    ( :METHOD ( ACHIEVE-POINTING0 ?SATELLITE0 ?DIRECTION1 )
              ( ( POINTING ?SATELLITE0 ?DIRECTION1 ) ( SATELLITE ?SATELLITE0 ) ( DIRECTION ?DIRECTION1 ) )
              ( )
    )
    ( :METHOD ( ACHIEVE-POINTING0 ?SATELLITE0 ?DIRECTION1 )
              ( ( NOT ( POINTING ?SATELLITE0 ?DIRECTION1 ) ) ( POINTING ?SATELLITE0 ?DIRECTION3 ) ( SATELLITE ?SATELLITE0 ) ( DIRECTION ?DIRECTION5 ) ( DIRECTION ?DIRECTION3 ) ( NOT ( VISITED-POINTING ?SATELLITE0 ?DIRECTION3 ) ) ( NOT ( VISITED-POINTING ?SATELLITE0 ?DIRECTION5 ) ) )
              ( ( !!VISIT-POINTING ?SATELLITE0 ?DIRECTION3 ) ( !TURN_TO ?SATELLITE0 ?DIRECTION5 ?DIRECTION3 ) ( ACHIEVE-POINTING0 ?SATELLITE0 ?DIRECTION1 ) )
    )
    ( :METHOD ( ACHIEVE-POWER_AVAIL ?SATELLITE0 )
              ( ( LOCKED-POWER_AVAIL ?SATELLITE0 ) ( SATELLITE ?SATELLITE0 )  )
              ( ( !!FLAG-POWER_AVAIL ?SATELLITE0 ) )
   )
    ( :METHOD ( ACHIEVE-POWER_AVAIL ?SATELLITE0 )
              ( ( POWER_AVAIL ?SATELLITE0 ) ( SATELLITE ?SATELLITE0 )  ( NOT ( LOCKED-POWER_AVAIL ?SATELLITE0 ) ) )
              ( ( !!LOCK-POWER_AVAIL ?SATELLITE0 ) )
    )
    ( :METHOD ( ACHIEVE-POWER_AVAIL ?SATELLITE0 )
              ( ( NOT ( ACHIEVING-POWER_AVAIL ?SATELLITE0 ) ) ( NOT ( POWER_AVAIL ?SATELLITE0 ) ) ( SATELLITE ?SATELLITE0 )  ( NOT ( LOCKED-POWER_AVAIL ?SATELLITE0 ) ) )
              ( ( !!ACHIEVE1 ?SATELLITE0 ) ( ACHIEVE-POWER_AVAIL1 ?SATELLITE0 ) ( !!LOCK-POWER_AVAIL ?SATELLITE0 ) ( !!STOPALL1 ?SATELLITE0 ) )
    )
    ( :METHOD ( ACHIEVE-POWER_AVAIL1 ?SATELLITE0 )
              ( ( POWER_AVAIL ?SATELLITE0 ) ( SATELLITE ?SATELLITE0 ) )
              ( )
    )
    ( :METHOD ( ACHIEVE-POWER_AVAIL1 ?SATELLITE0 )
              ( ( NOT ( POWER_AVAIL ?SATELLITE0 ) ) ( NOT ( POWER_AVAIL ?SATELLITE0 ) ) ( ON_BOARD ?INSTRUMENT2 ?SATELLITE0 ) ( INSTRUMENT ?INSTRUMENT2 ) ( SATELLITE ?SATELLITE0 ) ( NOT ( VISITED-POWER_AVAIL ?SATELLITE0 ) ) )
              ( ( DO-POWER_AVAIL-SWITCH_OFF1 ?INSTRUMENT2 ?SATELLITE0 ) ( ACHIEVE-POWER_AVAIL1 ?SATELLITE0 ) )
    )
    ( :METHOD ( ACHIEVE-POWER_ON ?INSTRUMENT0 )
              ( ( LOCKED-POWER_ON ?INSTRUMENT0 ) ( INSTRUMENT ?INSTRUMENT0 )  )
              ( ( !!FLAG-POWER_ON ?INSTRUMENT0 ) )
   )
    ( :METHOD ( ACHIEVE-POWER_ON ?INSTRUMENT0 )
              ( ( POWER_ON ?INSTRUMENT0 ) ( INSTRUMENT ?INSTRUMENT0 )  ( NOT ( LOCKED-POWER_ON ?INSTRUMENT0 ) ) )
              ( ( !!LOCK-POWER_ON ?INSTRUMENT0 ) )
    )
    ( :METHOD ( ACHIEVE-POWER_ON ?INSTRUMENT0 )
              ( ( NOT ( ACHIEVING-POWER_ON ?INSTRUMENT0 ) ) ( NOT ( POWER_ON ?INSTRUMENT0 ) ) ( INSTRUMENT ?INSTRUMENT0 )  ( NOT ( LOCKED-POWER_ON ?INSTRUMENT0 ) ) )
              ( ( !!ACHIEVE2 ?INSTRUMENT0 ) ( ACHIEVE-POWER_ON2 ?INSTRUMENT0 ) ( !!LOCK-POWER_ON ?INSTRUMENT0 ) ( !!STOPALL2 ?INSTRUMENT0 ) )
    )
    ( :METHOD ( ACHIEVE-POWER_ON2 ?INSTRUMENT0 )
              ( ( POWER_ON ?INSTRUMENT0 ) ( INSTRUMENT ?INSTRUMENT0 ) )
              ( )
    )
    ( :METHOD ( ACHIEVE-POWER_ON2 ?INSTRUMENT0 )
              ( ( NOT ( POWER_ON ?INSTRUMENT0 ) ) ( NOT ( POWER_ON ?INSTRUMENT0 ) ) ( ON_BOARD ?INSTRUMENT0 ?SATELLITE3 ) ( INSTRUMENT ?INSTRUMENT0 ) ( SATELLITE ?SATELLITE3 ) ( NOT ( VISITED-POWER_ON ?INSTRUMENT0 ) ) )
              ( ( DO-POWER_ON-SWITCH_ON2 ?INSTRUMENT0 ?SATELLITE3 ) ( ACHIEVE-POWER_ON2 ?INSTRUMENT0 ) )
    )
    ( :METHOD ( ACHIEVE-CALIBRATED ?INSTRUMENT0 )
              ( ( LOCKED-CALIBRATED ?INSTRUMENT0 ) ( INSTRUMENT ?INSTRUMENT0 )  )
              ( ( !!FLAG-CALIBRATED ?INSTRUMENT0 ) )
   )
    ( :METHOD ( ACHIEVE-CALIBRATED ?INSTRUMENT0 )
              ( ( CALIBRATED ?INSTRUMENT0 ) ( INSTRUMENT ?INSTRUMENT0 )  ( NOT ( LOCKED-CALIBRATED ?INSTRUMENT0 ) ) )
              ( ( !!LOCK-CALIBRATED ?INSTRUMENT0 ) )
    )
    ( :METHOD ( ACHIEVE-CALIBRATED ?INSTRUMENT0 )
              ( ( NOT ( ACHIEVING-CALIBRATED ?INSTRUMENT0 ) ) ( NOT ( CALIBRATED ?INSTRUMENT0 ) ) ( INSTRUMENT ?INSTRUMENT0 )  ( NOT ( LOCKED-CALIBRATED ?INSTRUMENT0 ) ) )
              ( ( !!ACHIEVE3 ?INSTRUMENT0 ) ( ACHIEVE-CALIBRATED3 ?INSTRUMENT0 ) ( !!LOCK-CALIBRATED ?INSTRUMENT0 ) ( !!STOPALL3 ?INSTRUMENT0 ) )
    )
    ( :METHOD ( ACHIEVE-CALIBRATED3 ?INSTRUMENT0 )
              ( ( CALIBRATED ?INSTRUMENT0 ) ( INSTRUMENT ?INSTRUMENT0 ) )
              ( )
    )
    ( :METHOD ( ACHIEVE-CALIBRATED3 ?INSTRUMENT0 )
              ( ( NOT ( CALIBRATED ?INSTRUMENT0 ) ) ( NOT ( CALIBRATED ?INSTRUMENT0 ) ) ( ON_BOARD ?INSTRUMENT0 ?SATELLITE2 ) ( CALIBRATION_TARGET ?INSTRUMENT0 ?DIRECTION4 ) ( SATELLITE ?SATELLITE2 ) ( INSTRUMENT ?INSTRUMENT0 ) ( DIRECTION ?DIRECTION4 ) ( NOT ( VISITED-CALIBRATED ?INSTRUMENT0 ) ) )
              ( ( DO-CALIBRATED-CALIBRATE3 ?SATELLITE2 ?INSTRUMENT0 ?DIRECTION4 ) ( ACHIEVE-CALIBRATED3 ?INSTRUMENT0 ) )
    )
    ( :METHOD ( ACHIEVE-HAVE_IMAGE ?DIRECTION0 ?MODE1 )
              ( ( LOCKED-HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ( DIRECTION ?DIRECTION0 )  ( MODE ?MODE1 )  )
              ( ( !!FLAG-HAVE_IMAGE ?DIRECTION0 ?MODE1 ) )
   )
    ( :METHOD ( ACHIEVE-HAVE_IMAGE ?DIRECTION0 ?MODE1 )
              ( ( HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ( DIRECTION ?DIRECTION0 )  ( MODE ?MODE1 )  ( NOT ( LOCKED-HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ) )
              ( ( !!LOCK-HAVE_IMAGE ?DIRECTION0 ?MODE1 ) )
    )
    ( :METHOD ( ACHIEVE-HAVE_IMAGE ?DIRECTION0 ?MODE1 )
              ( ( NOT ( ACHIEVING-HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ) ( NOT ( HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ) ( DIRECTION ?DIRECTION0 )  ( MODE ?MODE1 )  ( NOT ( LOCKED-HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ) )
              ( ( !!ACHIEVE4 ?DIRECTION0 ?MODE1 ) ( ACHIEVE-HAVE_IMAGE4 ?DIRECTION0 ?MODE1 ) ( !!LOCK-HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ( !!STOPALL4 ?DIRECTION0 ?MODE1 ) )
    )
    ( :METHOD ( ACHIEVE-HAVE_IMAGE4 ?DIRECTION0 ?MODE1 )
              ( ( HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ( DIRECTION ?DIRECTION0 ) ( MODE ?MODE1 ) )
              ( )
    )
    ( :METHOD ( ACHIEVE-HAVE_IMAGE4 ?DIRECTION0 ?MODE1 )
              ( ( NOT ( HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ) ( NOT ( HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ) ( ON_BOARD ?INSTRUMENT6 ?SATELLITE4 ) ( SUPPORTS ?INSTRUMENT6 ?MODE1 ) ( SATELLITE ?SATELLITE4 ) ( DIRECTION ?DIRECTION0 ) ( INSTRUMENT ?INSTRUMENT6 ) ( MODE ?MODE1 ) ( NOT ( VISITED-HAVE_IMAGE ?DIRECTION0 ?MODE1 ) ) )
              ( ( DO-HAVE_IMAGE-TAKE_IMAGE4 ?SATELLITE4 ?DIRECTION0 ?INSTRUMENT6 ?MODE1 ) ( ACHIEVE-HAVE_IMAGE4 ?DIRECTION0 ?MODE1 ) )
    )
))
