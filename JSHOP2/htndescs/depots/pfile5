)
(:htn
	:tasks (and
	(task0 (ACHIEVE-ON CRATE0 CRATE5))
	(task1 (ACHIEVE-ON CRATE1 PALLET1))
	(task2 (ACHIEVE-ON CRATE2 CRATE0))
	(task3 (ACHIEVE-ON CRATE3 PALLET2))
	(task4 (ACHIEVE-ON CRATE4 CRATE6))
